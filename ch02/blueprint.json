{
    "name": "Integration Instagram",
    "flow": [
        {
            "id": 1,
            "module": "instagram:WatchMedia",
            "version": 4,
            "parameters": {
                "limit": 2,
                "__IMTCONN__": 13794
            },
            "mapper": {},
            "metadata": {
                "designer": {
                    "x": 0,
                    "y": 0
                },
                "restore": {
                    "parameters": {
                        "__IMTCONN__": {
                            "data": {
                                "scoped": "true",
                                "connection": "instagram2"
                            },
                            "label": "My Instagram connection (meeinoffice)"
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "__IMTCONN__",
                        "type": "account:instagram2",
                        "label": "Connection",
                        "required": true
                    },
                    {
                        "name": "limit",
                        "type": "uinteger",
                        "label": "Limit",
                        "required": true
                    }
                ]
            }
        },
        {
            "id": 2,
            "module": "line:sendANotification",
            "version": 1,
            "parameters": {
                "__IMTCONN__": 17084
            },
            "filter": {
                "name": "阻擋短片",
                "conditions": [
                    [
                        {
                            "a": "{{1.media_type}}",
                            "b": "VIDEO",
                            "o": "text:notequal"
                        }
                    ]
                ]
            },
            "mapper": {
                "select": "url",
                "message": "{{1.caption}}\n{{1.permalink}}",
                "stickerId": "52114146",
                "addSticker": true,
                "imageFullsize": "{{1.media_url}}",
                "imageThumbnail": "{{1.media_url}}",
                "stickerPackageId": "11539",
                "notificationDisabled": false
            },
            "metadata": {
                "designer": {
                    "x": 300,
                    "y": 0
                },
                "restore": {
                    "expect": {
                        "select": {
                            "label": "By URL"
                        },
                        "addSticker": {
                            "mode": "chose"
                        },
                        "notificationDisabled": {
                            "mode": "chose"
                        }
                    },
                    "parameters": {
                        "__IMTCONN__": {
                            "data": {
                                "scoped": "true",
                                "connection": "line"
                            },
                            "label": "My LINE (Notify API) connection"
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "__IMTCONN__",
                        "type": "account:line",
                        "label": "Connection",
                        "required": true
                    }
                ],
                "expect": [
                    {
                        "name": "message",
                        "type": "text",
                        "label": "Message",
                        "required": true,
                        "validate": {
                            "max": 1000
                        }
                    },
                    {
                        "name": "select",
                        "type": "select",
                        "label": "Upload an Image",
                        "validate": {
                            "enum": [
                                "url",
                                "file"
                            ]
                        }
                    },
                    {
                        "name": "addSticker",
                        "type": "boolean",
                        "label": "Add a Sticker",
                        "required": true
                    },
                    {
                        "name": "notificationDisabled",
                        "type": "boolean",
                        "label": "Disable a Push Notification",
                        "required": true
                    },
                    {
                        "name": "imageFullsize",
                        "type": "url",
                        "label": "Image Full Size URL",
                        "required": true
                    },
                    {
                        "name": "imageThumbnail",
                        "type": "url",
                        "label": "Image Thumbnail URL",
                        "required": true
                    },
                    {
                        "name": "stickerPackageId",
                        "type": "uinteger",
                        "label": "Sticker Package ID",
                        "required": true
                    },
                    {
                        "name": "stickerId",
                        "type": "uinteger",
                        "label": "Sticker ID",
                        "required": true
                    }
                ]
            }
        }
    ],
    "metadata": {
        "instant": false,
        "version": 1,
        "scenario": {
            "roundtrips": 1,
            "maxErrors": 3,
            "autoCommit": true,
            "autoCommitTriggerLast": true,
            "sequential": false,
            "slots": null,
            "confidential": false,
            "dataloss": false,
            "dlq": false,
            "freshVariables": false
        },
        "designer": {
            "orphans": []
        },
        "zone": "us2.make.com"
    }
}